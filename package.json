{
  "name": "bn-erp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:libs": "nest build libs",
    "build:bn-erp": "nest build bn-erp",
    "build:inventory-service": "nest build inventory-service",
    "build:bunny-cloud-service": "nest build bunny-cloud-service",
    "build:user-service": "nest build user-service",
    "build:order-service": "nest build order-service",
    "build:all": "npm run build:bn-erp && npm run build:inventory-service && npm run build:bunny-cloud-service && npm run build:user-service && npm run build:order-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/bn-erp/src/main.ts",
    "start:debug": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/bn-erp/src/main.ts --debug",
    "start:prod": "node dist/apps/bn-erp/main",
    "start:inventory:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/inventory-service/src/main.ts",
    "start:inventory:debug": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/inventory-service/src/main.ts --debug",
    "start:inventory:prod": "node dist/apps/inventory-service/main",
    "seed:inventory": "ts-node -r tsconfig-paths/register apps/inventory-service/src/database/seeds/seed.runner.ts",
    "start:bunny-cloud:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/bunny-cloud-service/src/main.ts",
    "start:bunny-cloud:debug": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/bunny-cloud-service/src/main.ts --debug",
    "start:bunny-cloud:prod": "node dist/apps/bunny-cloud-service/main",
    "start:user:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/user-service/src/main.ts",
    "start:user:debug": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/user-service/src/main.ts --debug",
    "start:user:prod": "node dist/apps/user-service/main",
    "start:lead:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/lead-service/src/main.ts",
    "start:lead:debug": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/lead-service/src/main.ts --debug",
    "start:lead:prod": "node dist/apps/lead-service/main",
    "start:order:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/order-service/src/main.ts",
    "start:order:debug": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register apps/order-service/src/main.ts --debug",
    "start:order:prod": "node dist/apps/order-service/main",
    "seed:user": "ts-node -r tsconfig-paths/register apps/user-service/src/database/seeds/seed.runner.ts",
    "seed:lead": "ts-node -r tsconfig-paths/register apps/lead-service/src/database/seeds/seed.runner.ts",
    "seed:order": "ts-node -r tsconfig-paths/register apps/order-service/src/database/seeds/seed.runner.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/bn-erp/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "http-proxy-middleware": "^3.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.26",
    "typeorm-extension": "^3.7.1",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
